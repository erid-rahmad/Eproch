<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <!--
        Added the constraints for entity MContractLine.
    -->
    <changeSet id="20210625061000-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="contract_id"
                                 baseTableName="m_contract_line"
                                 constraintName="fk_m_contract_line_contract_id"
                                 referencedColumnNames="id"
                                 referencedTableName="m_contract"/>

        <addForeignKeyConstraint baseColumnNames="ad_organization_id"
                                 baseTableName="m_contract_line"
                                 constraintName="fk_m_contract_line_ad_organization_id"
                                 referencedColumnNames="id"
                                 referencedTableName="ad_organization"/>

        <addForeignKeyConstraint baseColumnNames="cost_center_id"
                                 baseTableName="m_contract_line"
                                 constraintName="fk_m_contract_line_cost_center_id"
                                 referencedColumnNames="id"
                                 referencedTableName="c_cost_center"/>

        <addForeignKeyConstraint baseColumnNames="product_id"
                                 baseTableName="m_contract_line"
                                 constraintName="fk_m_contract_line_product_id"
                                 referencedColumnNames="id"
                                 referencedTableName="c_product"/>

        <addForeignKeyConstraint baseColumnNames="uom_id"
                                 baseTableName="m_contract_line"
                                 constraintName="fk_m_contract_line_uom_id"
                                 referencedColumnNames="id"
                                 referencedTableName="c_unit_of_measure"/>

    </changeSet>
    <changeSet id="20280813730123" author="erid">
        <addForeignKeyConstraint baseColumnNames="vendor_id"
                                 baseTableName="m_contract_line"
                                 constraintName="fk_m_contract_line_vendor_id"
                                 referencedColumnNames="id"
                                 referencedTableName="c_vendor"/>
    </changeSet>
</databaseChangeLog>
