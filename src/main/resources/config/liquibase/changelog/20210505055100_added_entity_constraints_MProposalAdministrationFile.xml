<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <!--
        Added the constraints for entity MProposalAdministrationFile.
    -->
    <changeSet id="20210505055100-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="c_attachment_id"
                                 baseTableName="m_proposal_administration_file"
                                 constraintName="fk_m_proposal_administration_file_c_attachment_id"
                                 referencedColumnNames="id"
