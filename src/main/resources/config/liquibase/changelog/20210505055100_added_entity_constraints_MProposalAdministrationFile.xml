        <addForeignKeyConstraint baseColumnNames="bidding_sub_criteria_id"
                                 baseTableName="m_proposal_administration_file"
                                 constraintName="fk_m_proposal_administration_file_bidding_sub_criteria_id"
                                 referencedColumnNames="id"
