<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <!--
        Added the constraints for entity MQuoteSupplier.
    -->
    <changeSet id="20210814083500-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="quotation_id"
                                 baseTableName="m_quote_supplier"
                                 constraintName="fk_m_quote_supplier_quotation_id"
