<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <!--
        Added the constraints for entity MVendorConfirmation.
    -->
    <changeSet id="20210526093400-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="bidding_id"
                                 baseTableName="m_vendor_confirmation"
                                 constraintName="fk_m_vendor_confirmation_bidding_id"
                                 referencedColumnNames="id"
                                 referencedTableName="m_bidding"/>

        <addForeignKeyConstraint baseColumnNames="ad_organization_id"
                                 baseTableName="m_vendor_confirmation"
                                 constraintName="fk_m_vendor_confirmation_ad_organization_id"
                                 referencedColumnNames="id"
                                 referencedTableName="ad_organization"/>

        <addForeignKeyConstraint baseColumnNames="currency_id"
                                 baseTableName="m_vendor_confirmation"
                                 constraintName="fk_m_vendor_confirmation_currency_id"
                                 referencedColumnNames="id"
                                 referencedTableName="c_currency"/>

        <addForeignKeyConstraint baseColumnNames="cost_center_id"
                                 baseTableName="m_vendor_confirmation"
                                 constraintName="fk_m_vendor_confirmation_cost_center_id"
                                 referencedColumnNames="id"
                                 referencedTableName="c_cost_center"/>

        <addForeignKeyConstraint baseColumnNames="pic_user_id"
                                 baseTableName="m_vendor_confirmation"
                                 constraintName="fk_m_vendor_confirmation_pic_user_id"
                                 referencedColumnNames="user_id"
                                 referencedTableName="ad_user"/>

    </changeSet>
</databaseChangeLog>
