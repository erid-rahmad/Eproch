<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <!--
        Added the constraints for entity MContractTask.
    -->
    <changeSet id="20210728103600-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="ad_organization_id"
                                 baseTableName="m_contract_task"
                                 constraintName="fk_m_contract_task_ad_organization_id"
                                 referencedColumnNames="id"
                                 referencedTableName="ad_organization"/>

        <addForeignKeyConstraint baseColumnNames="contract_id"
                                 baseTableName="m_contract_task"
                                 constraintName="fk_m_contract_task_contract_id"
                                 referencedColumnNames="id"
                                 referencedTableName="m_contract"/>

        <addForeignKeyConstraint baseColumnNames="task_id"
                                 baseTableName="m_contract_task"
                                 constraintName="fk_m_contract_task_task_id"
                                 referencedColumnNames="id"
                                 referencedTableName="c_task"/>

    </changeSet>
</databaseChangeLog>
