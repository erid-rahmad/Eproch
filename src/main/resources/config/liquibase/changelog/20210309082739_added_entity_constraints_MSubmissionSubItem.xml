<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <!--
        Added the constraints for entity MSubmissionSubItem.
    -->
    <changeSet id="20210309082739-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="bidding_sub_item_id"
                                 baseTableName="m_submission_sub_item"
                                 constraintName="fk_m_submission_sub_item_bidding_sub_item_id"
