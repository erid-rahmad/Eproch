<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <!--
        Added the constraints for entity MAuctionItem.
    -->
    <changeSet id="20210524032100-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="ad_organization_id"
                                 baseTableName="m_auction_item"
                                 constraintName="fk_m_auction_item_ad_organization_id"
                                 referencedColumnNames="id"
                                 referencedTableName="ad_organization"/>

        <addForeignKeyConstraint baseColumnNames="auction_id"
                                 baseTableName="m_auction_item"
                                 constraintName="fk_m_auction_item_auction_id"
                                 referencedColumnNames="id"
                                 referencedTableName="m_auction"/>

        <addForeignKeyConstraint baseColumnNames="product_id"
                                 baseTableName="m_auction_item"
                                 constraintName="fk_m_auction_item_product_id"
                                 referencedColumnNames="id"
                                 referencedTableName="c_product"/>

        <addForeignKeyConstraint baseColumnNames="uom_id"
                                 baseTableName="m_auction_item"
                                 constraintName="fk_m_auction_item_uom_id"
                                 referencedColumnNames="id"
                                 referencedTableName="c_unit_of_measure"/>

    </changeSet>
</databaseChangeLog>
