<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <!--
        Added the constraints for entity MPrequalificationSchedule.
    -->
    <changeSet id="20210716035000-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="prequalification_id"
                                 baseTableName="m_prequalification_schedule"
                                 constraintName="fk_m_prequalification_schedule_prequalification_id"
                                 referencedColumnNames="id"
                                 referencedTableName="m_prequalification_information"/>

        <addForeignKeyConstraint baseColumnNames="ad_organization_id"
                                 baseTableName="m_prequalification_schedule"
                                 constraintName="fk_m_prequalification_schedule_ad_organization_id"
                                 referencedColumnNames="id"
                                 referencedTableName="ad_organization"/>

        <addForeignKeyConstraint baseColumnNames="event_line_id"
                                 baseTableName="m_prequalification_schedule"
                                 constraintName="fk_m_prequalification_schedule_event_line_id"
                                 referencedColumnNames="id"
                                 referencedTableName="c_prequalification_event_line"/>

    </changeSet>

    <changeSet id="20210716035000-dateset" author="ricky-k">

        <addForeignKeyConstraint baseColumnNames="date_set_id"
                                 baseTableName="m_prequalification_schedule"
                                 constraintName="fk_m_prequalification_schedule_date_set_id"
                                 referencedColumnNames="id"
                                 referencedTableName="m_prequalification_date_set"/>

    </changeSet>
</databaseChangeLog>
