<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <!--
        Added the constraints for entity MBlacklist.
    -->
    <changeSet id="20210712043900-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="attachment_id"
                                 baseTableName="m_blacklist"
                                 constraintName="fk_m_blacklist_attachment_id"
                                 referencedColumnNames="id"
                                 referencedTableName="c_attachment"/>

        <addForeignKeyConstraint baseColumnNames="ad_organization_id"
                                 baseTableName="m_blacklist"
                                 constraintName="fk_m_blacklist_ad_organization_id"
                                 referencedColumnNames="id"
                                 referencedTableName="ad_organization"/>

        <addForeignKeyConstraint baseColumnNames="vendor_id"
                                 baseTableName="m_blacklist"
                                 constraintName="fk_m_blacklist_vendor_id"
                                 referencedColumnNames="id"
                                 referencedTableName="c_vendor"/>

        <addForeignKeyConstraint baseColumnNames="business_category_id"
                                 baseTableName="m_blacklist"
                                 constraintName="fk_m_blacklist_business_category_id"
                                 referencedColumnNames="id"
                                 referencedTableName="c_business_category"/>

        <addForeignKeyConstraint baseColumnNames="sub_business_category_id"
                                 baseTableName="m_blacklist"
                                 constraintName="fk_m_blacklist_sub_business_category_id"
                                 referencedColumnNames="id"
                                 referencedTableName="c_business_category"/>

    </changeSet>
</databaseChangeLog>
