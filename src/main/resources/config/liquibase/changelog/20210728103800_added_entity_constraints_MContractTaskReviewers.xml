        <addForeignKeyConstraint baseColumnNames="pic_id"
                                 baseTableName="m_contract_task_reviewers"
                                 constraintName="fk_m_contract_task_reviewers_pic_id"
                                 referencedColumnNames="user_id"
                                 referencedTableName="ad_user"/>

    </changeSet>
</databaseChangeLog>
