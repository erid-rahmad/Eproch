<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <!--
        Added the constraints for entity MContractTaskReviewers.
    -->
    <changeSet id="20210728103800-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="ad_organization_id"
                                 baseTableName="m_contract_task_reviewers"
                                 constraintName="fk_m_contract_task_reviewers_ad_organization_id"
                                 referencedColumnNames="id"
                                 referencedTableName="ad_organization"/>

        <addForeignKeyConstraint baseColumnNames="contract_task_id"
                                 baseTableName="m_contract_task_reviewers"
                                 constraintName="fk_m_contract_task_reviewers_contract_task_id"
                                 referencedColumnNames="id"
