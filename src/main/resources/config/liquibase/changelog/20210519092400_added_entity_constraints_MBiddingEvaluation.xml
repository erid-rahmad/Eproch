<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <!--
        Added the constraints for entity MBiddingEvaluation.
    -->
    <changeSet id="20210519092400-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="bidding_submission_id"
                                 baseTableName="m_bidding_evaluation"
                                 constraintName="fk_m_bidding_evaluation_bidding_submission_id"
                                 referencedColumnNames="id"
                                 referencedTableName="m_bidding_submission"/>

        <addForeignKeyConstraint baseColumnNames="bidding_schedule_id"
                                 baseTableName="m_bidding_evaluation"
                                 constraintName="fk_m_bidding_evaluation_bidding_schedule_id"
                                 referencedColumnNames="id"
                                 referencedTableName="m_bidding_schedule"/>

        <addForeignKeyConstraint baseColumnNames="ad_organization_id"
                                 baseTableName="m_bidding_evaluation"
                                 constraintName="fk_m_bidding_evaluation_ad_organization_id"
                                 referencedColumnNames="id"
                                 referencedTableName="ad_organization"/>

    </changeSet>
</databaseChangeLog>
