<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <!--
        Added the constraints for entity MProposalPrice.
    -->
    <changeSet id="20210505055200-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="bidding_submission_id"
                                 baseTableName="m_proposal_price"
                                 constraintName="fk_m_proposal_price_bidding_submission_id"
                                 referencedColumnNames="id"
                                 referencedTableName="m_bidding_submission"/>

        <addForeignKeyConstraint baseColumnNames="ad_organization_id"
                                 baseTableName="m_proposal_price"
                                 constraintName="fk_m_proposal_price_ad_organization_id"
                                 referencedColumnNames="id"
                                 referencedTableName="ad_organization"/>

    </changeSet>
    <changeSet id="20210609114900" author="erid">
        <addForeignKeyConstraint baseColumnNames="attachment_id"
                                 baseTableName="m_proposal_price"
                                 constraintName="fk_m_proposal_price_attachment_id"
                                 referencedColumnNames="id"
                                 referencedTableName="c_attachment"/>
    </changeSet>
</databaseChangeLog>
