<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <!--
        Added the constraints for entity MBiddingNegotiation.
    -->
    <changeSet id="20210602063400-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="bidding_eval_id"
                                 baseTableName="m_bidding_negotiation"
                                 constraintName="fk_m_bidding_negotiation_bidding_eval_id"
                                 referencedColumnNames="id"
                                 referencedTableName="m_bidding_evaluation"/>

        <addForeignKeyConstraint baseColumnNames="ad_organization_id"
                                 baseTableName="m_bidding_negotiation"
                                 constraintName="fk_m_bidding_negotiation_ad_organization_id"
                                 referencedColumnNames="id"
                                 referencedTableName="ad_organization"/>

        <addForeignKeyConstraint baseColumnNames="bidding_schedule_id"
                                 baseTableName="m_bidding_negotiation"
                                 constraintName="fk_m_bidding_negotiation_bidding_schedule_id"
                                 referencedColumnNames="id"
                                 referencedTableName="m_bidding_schedule"/>

    </changeSet>

    <changeSet id="202106120012" author="erid">
        <addForeignKeyConstraint baseColumnNames="bidding_eval_result_id"
                                 baseTableName="m_bidding_negotiation"
                                 constraintName="fk_m_bidding_negotiation_bidding_eval_result_id"
                                 referencedColumnNames="id"
                                 referencedTableName="m_bidding_eval_result"/>

    </changeSet>
</databaseChangeLog>
