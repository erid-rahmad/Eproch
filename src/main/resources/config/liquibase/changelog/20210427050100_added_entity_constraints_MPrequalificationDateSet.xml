<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <!--
        Added the constraints for entity MPrequalificationDateSet.
    -->
    <changeSet id="20210427050100-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="bidding_schedule_id"
                                 baseTableName="m_prequalification_date_set"
                                 constraintName="fk_m_prequalification_date_set_bidding_schedule_id"
                                 referencedColumnNames="id"
                                 referencedTableName="m_bidding_schedule"/>

        <addForeignKeyConstraint baseColumnNames="ad_organization_id"
                                 baseTableName="m_prequalification_date_set"
                                 constraintName="fk_m_prequalification_date_set_ad_organization_id"
                                 referencedColumnNames="id"
                                 referencedTableName="ad_organization"/>

    </changeSet>

    <changeSet id="20210519150000" author="Ananta Aryadewa">
        <dropForeignKeyConstraint baseTableName="m_prequalification_date_set" constraintName="fk_m_prequalification_date_set_ad_organization_id" />
        <dropColumn tableName="m_prequalification_date_set" columnName="bidding_schedule_id"/>
    </changeSet>
</databaseChangeLog>
