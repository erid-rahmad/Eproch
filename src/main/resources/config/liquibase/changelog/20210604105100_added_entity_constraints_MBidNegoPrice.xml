<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <!--
        Added the constraints for entity MBidNegoPrice.
    -->
    <changeSet id="20210604105100-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="bidding_id"
                                 baseTableName="m_bid_nego_price"
                                 constraintName="fk_m_bid_nego_price_bidding_id"
                                 referencedColumnNames="id"
                                 referencedTableName="m_bidding"/>

        <addForeignKeyConstraint baseColumnNames="vendor_invitation_id"
                                 baseTableName="m_bid_nego_price"
                                 constraintName="fk_m_bid_nego_price_vendor_invitation_id"
                                 referencedColumnNames="id"
                                 referencedTableName="m_vendor_invitation"/>

        <addForeignKeyConstraint baseColumnNames="price_proposal_id"
                                 baseTableName="m_bid_nego_price"
                                 constraintName="fk_m_bid_nego_price_price_proposal_id"
                                 referencedColumnNames="id"
                                 referencedTableName="m_proposal_price"/>

        <addForeignKeyConstraint baseColumnNames="negotiation_line_id"
                                 baseTableName="m_bid_nego_price"
                                 constraintName="fk_m_bid_nego_price_negotiation_line_id"
                                 referencedColumnNames="id"
                                 referencedTableName="m_bidding_negotiation_line"/>

    </changeSet>
</databaseChangeLog>
