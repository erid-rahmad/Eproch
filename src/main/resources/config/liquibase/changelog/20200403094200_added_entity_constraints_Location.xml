<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <!--
        Added the constraints for entity Location.
    -->
    <changeSet id="20200403094200-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="city_id"
                                 baseTableName="location"
                                 constraintName="fk_location_city_id"
                                 referencedColumnNames="id"
                                 referencedTableName="city"/>

        <addForeignKeyConstraint baseColumnNames="vendor_id"
                                 baseTableName="location"
                                 constraintName="fk_location_vendor_id"
                                 referencedColumnNames="id"
                                 referencedTableName="vendor"/>

    </changeSet>
</databaseChangeLog>
