<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <!--
        Added the constraints for entity MAuctionParticipant.
    -->
    <changeSet id="20210524032000-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="ad_organization_id"
                                 baseTableName="m_auction_participant"
                                 constraintName="fk_m_auction_participant_ad_organization_id"
                                 referencedColumnNames="id"
                                 referencedTableName="ad_organization"/>

        <addForeignKeyConstraint baseColumnNames="auction_id"
                                 baseTableName="m_auction_participant"
                                 constraintName="fk_m_auction_participant_auction_id"
                                 referencedColumnNames="id"
                                 referencedTableName="m_auction"/>

        <addForeignKeyConstraint baseColumnNames="user_user_id"
                                 baseTableName="m_auction_participant"
                                 constraintName="fk_m_auction_participant_user_id"
                                 referencedColumnNames="user_id"
                                 referencedTableName="ad_user"/>

        <addForeignKeyConstraint baseColumnNames="vendor_id"
                                 baseTableName="m_auction_participant"
                                 constraintName="fk_m_auction_participant_vendor_id"
                                 referencedColumnNames="id"
                                 referencedTableName="c_vendor"/>

    </changeSet>
</databaseChangeLog>
