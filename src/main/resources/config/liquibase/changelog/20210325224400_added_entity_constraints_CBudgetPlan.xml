<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <!--
        Added the constraints for entity CBudgetPlan.
    -->
    <changeSet id="20210325224400-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="ad_organization_id"
                                 baseTableName="c_budget_plan"
                                 constraintName="fk_c_budget_plan_ad_organization_id"
                                 referencedColumnNames="id"
                                 referencedTableName="ad_organization"/>

        <addForeignKeyConstraint baseColumnNames="c_cost_center_id"
                                 baseTableName="c_budget_plan"
                                 constraintName="fk_c_budget_plan_c_cost_center_id"
                                 referencedColumnNames="id"
                                 referencedTableName="c_cost_center"/>

        <addForeignKeyConstraint baseColumnNames="c_currency_id"
                                 baseTableName="c_budget_plan"
                                 constraintName="fk_c_budget_plan_c_currency_id"
                                 referencedColumnNames="id"
                                 referencedTableName="c_currency"/>

    </changeSet>
</databaseChangeLog>
