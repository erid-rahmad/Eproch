<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <!--
        Added the constraints for entity MPrequalificationSubmission.
    -->
    <changeSet id="20210721050800-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="ad_organization_id"
                                 baseTableName="m_prequalification_submission"
                                 constraintName="fk_m_prequalification_submission_ad_organization_id"
                                 referencedColumnNames="id"
                                 referencedTableName="ad_organization"/>

        <addForeignKeyConstraint baseColumnNames="prequalification_id"
                                 baseTableName="m_prequalification_submission"
                                 constraintName="fk_m_prequalification_submission_prequalification_id"
                                 referencedColumnNames="id"
                                 referencedTableName="m_prequalification_information"/>

        <addForeignKeyConstraint baseColumnNames="document_type_id"
                                 baseTableName="m_prequalification_submission"
                                 constraintName="fk_m_prequalification_submission_document_type_id"
                                 referencedColumnNames="id"
                                 referencedTableName="c_document_type"/>

        <addForeignKeyConstraint baseColumnNames="vendor_id"
                                 baseTableName="m_prequalification_submission"
                                 constraintName="fk_m_prequalification_submission_vendor_id"
                                 referencedColumnNames="id"
                                 referencedTableName="c_vendor"/>

    </changeSet>
</databaseChangeLog>
