<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <!--
        Added the constraints for entity CEvaluationMethod.
    -->
    <changeSet id="20210321210700-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="ad_organization_id"
                                 baseTableName="c_evaluation_method"
                                 constraintName="fk_c_evaluation_method_ad_organization_id"
                                 referencedColumnNames="id"
                                 referencedTableName="ad_organization"/>

        <addForeignKeyConstraint baseColumnNames="bidding_type_id"
                                 baseTableName="c_evaluation_method"
                                 constraintName="fk_c_evaluation_method_bidding_type_id"
                                 referencedColumnNames="id"
                                 referencedTableName="c_bidding_type"/>

    </changeSet>
    <changeSet id="20210416210700" author="">
        <addForeignKeyConstraint baseColumnNames="event_type_id"
                                 baseTableName="c_evaluation_method"
                                 constraintName="fk_c_evaluation_method_event_type_id"
                                 referencedColumnNames="id"
                                 referencedTableName="c_event_type"/>
    </changeSet>
</databaseChangeLog>
