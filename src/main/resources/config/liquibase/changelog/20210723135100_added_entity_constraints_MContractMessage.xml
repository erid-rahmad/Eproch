<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <!--
        Added the constraints for entity MContractMessage.
    -->
    <changeSet id="20210723135100-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="ad_organization_id"
                                 baseTableName="m_contract_message"
                                 constraintName="fk_m_contract_message_ad_organization_id"
                                 referencedColumnNames="id"
                                 referencedTableName="ad_organization"/>

        <addForeignKeyConstraint baseColumnNames="contract_id"
                                 baseTableName="m_contract_message"
                                 constraintName="fk_m_contract_message_contract_id"
                                 referencedColumnNames="id"
                                 referencedTableName="m_contract"/>

        <addForeignKeyConstraint baseColumnNames="vendor_id"
                                 baseTableName="m_contract_message"
                                 constraintName="fk_m_contract_message_vendor_id"
                                 referencedColumnNames="id"
                                 referencedTableName="c_vendor"/>

        <addForeignKeyConstraint baseColumnNames="attachment_id"
                                 baseTableName="m_contract_message"
                                 constraintName="fk_m_contract_message_attachment_id"
                                 referencedColumnNames="id"
                                 referencedTableName="c_attachment"/>

    </changeSet>
</databaseChangeLog>
