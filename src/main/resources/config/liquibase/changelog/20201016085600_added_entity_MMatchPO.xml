            <column name="c_cost_center" type="string"/>
            <column name="po_no" type="string"/>
            <column name="po_date" type="date"/>
            <column name="receipt_no" type="string"/>
            <column name="receipt_date" type="date"/>
            <column name="delivery_no" type="string"/>
            <column name="m_product_code" type="string"/>
            <column name="m_product_name" type="string"/>
            <column name="m_product_desc" type="string"/>
            <column name="c_uom" type="string"/>
            <column name="qty" type="string"/>
            <column name="c_currency" type="string"/>
            <column name="c_conversion_rate" type="string"/>
            <column name="open_qty" type="string"/>
            <column name="price_actual" type="string"/>
            <column name="foreign_actual" type="string"/>
            <column name="open_amount" type="string"/>
            <column name="open_foreign_amount" type="string"/>
            <column name="total_lines" type="string"/>
            <column name="foreign_total_lines" type="string"/>
            <column name="c_tax" type="string"/>
            <column name="tax_amount" type="string"/>
            <column name="foreign_tax_amount" type="string"/>
            <column name="m_locator" type="string"/>
            <column name="ad_organization" type="string"/>
            <!-- jhipster-needle-liquibase-add-loadcolumn - JHipster (and/or extensions) can add load columns here, do not remove-->
        </loadData>
    </changeSet>

</databaseChangeLog>
