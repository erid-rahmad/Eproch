<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <!--
        Added the constraints for entity CAnnouncementResult.
    -->
    <changeSet id="20210520050500-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="ad_organization_id"
                                 baseTableName="c_announcement_result"
                                 constraintName="fk_c_announcement_result_ad_organization_id"
                                 referencedColumnNames="id"
                                 referencedTableName="ad_organization"/>

        <addForeignKeyConstraint baseColumnNames="bidding_id"
                                 baseTableName="c_announcement_result"
                                 constraintName="fk_c_announcement_result_bidding_id"
                                 referencedColumnNames="id"
                                 referencedTableName="m_bidding"/>

        <addForeignKeyConstraint baseColumnNames="bidding_schedule_id"
                                 baseTableName="c_announcement_result"
                                 constraintName="fk_c_announcement_result_bidding_schedule_id"
                                 referencedColumnNames="id"
                                 referencedTableName="m_bidding_schedule"/>

        <addForeignKeyConstraint baseColumnNames="attachment_id"
                                 baseTableName="c_announcement_result"
                                 constraintName="fk_c_announcement_result_attachment_id"
                                 referencedColumnNames="id"
                                 referencedTableName="c_attachment"/>

    </changeSet>
</databaseChangeLog>
