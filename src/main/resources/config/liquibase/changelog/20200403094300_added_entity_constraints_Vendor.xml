<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <!--
        Added the constraints for entity Vendor.
    -->
    <changeSet id="20200403094300-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="location_id"
                                 baseTableName="vendor"
                                 constraintName="fk_vendor_location_id"
                                 referencedColumnNames="id"
                                 referencedTableName="location"/>

        <addForeignKeyConstraint baseColumnNames="vendor_id"
                                 baseTableName="vendor_business_category"
                                 constraintName="fk_vendor_business_category_vendor_id"
                                 referencedColumnNames="id"
                                 referencedTableName="vendor"/>

        <addForeignKeyConstraint baseColumnNames="business_category_id"
                                 baseTableName="vendor_business_category"
                                 constraintName="fk_vendor_business_category_business_category_id"
                                 referencedColumnNames="id"
                                 referencedTableName="business_category"/>
        
    </changeSet>
</databaseChangeLog>
