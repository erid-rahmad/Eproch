<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <!--
        Added the constraints for entity MBiddingResult.
    -->
    <changeSet id="20210520080200-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="ad_organization_id"
                                 baseTableName="m_bidding_result"
                                 constraintName="fk_m_bidding_result_ad_organization_id"
                                 referencedColumnNames="id"
                                 referencedTableName="ad_organization"/>

        <addForeignKeyConstraint baseColumnNames="announcement_result_id"
                                 baseTableName="m_bidding_result"
                                 constraintName="fk_m_bidding_result_announcement_result_id"
                                 referencedColumnNames="id"
                                 referencedTableName="c_announcement_result"/>

        <addForeignKeyConstraint baseColumnNames="bidding_id"
                                 baseTableName="m_bidding_result"
                                 constraintName="fk_m_bidding_result_bidding_id"
                                 referencedColumnNames="id"
                                 referencedTableName="m_bidding"/>

        <addForeignKeyConstraint baseColumnNames="vendor_id"
                                 baseTableName="m_bidding_result"
                                 constraintName="fk_m_bidding_result_vendor_id"
                                 referencedColumnNames="id"
                                 referencedTableName="c_vendor"/>

        <addForeignKeyConstraint baseColumnNames="bidding_eval_result_id"
                                 baseTableName="m_bidding_result"
                                 constraintName="fk_m_bidding_result_bidding_eval_result_id"
                                 referencedColumnNames="id"
                                 referencedTableName="m_bidding_eval_result"/>

    </changeSet>
</databaseChangeLog>
