<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <!--
        Added the constraints for entity MBlacklistLine.
    -->
    <changeSet id="20210712044000-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="ad_organization_id"
                                 baseTableName="m_blacklist_line"
                                 constraintName="fk_m_blacklist_line_ad_organization_id"
                                 referencedColumnNames="id"
                                 referencedTableName="ad_organization"/>

        <addForeignKeyConstraint baseColumnNames="pic_user_id"
                                 baseTableName="m_blacklist_line"
                                 constraintName="fk_m_blacklist_line_pic_user_id"
                                 referencedColumnNames="user_id"
                                 referencedTableName="ad_user"/>

        <addForeignKeyConstraint baseColumnNames="functionary_id"
                                 baseTableName="m_blacklist_line"
                                 constraintName="fk_m_blacklist_line_functionary_id"
                                 referencedColumnNames="id"
                                 referencedTableName="c_functionary"/>

    </changeSet>

    <changeSet id="20210712044000-header-relation" author="ricky-k">
        <addForeignKeyConstraint baseColumnNames="blacklist_id"
                                 baseTableName="m_blacklist_line"
                                 constraintName="fk_m_blacklist_line_blacklist_id"
                                 referencedColumnNames="id"
                                 referencedTableName="m_blacklist"/>
    </changeSet>                            
</databaseChangeLog>
