<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <!--
        Added the constraints for entity MWarningLetter.
    -->
    <changeSet id="20210707092500-2" author="jhipster">
        <addForeignKeyConstraint baseColumnNames="ad_organization_id"
                                 baseTableName="m_warning_letter"
                                 constraintName="fk_m_warning_letter_ad_organization_id"
                                 referencedColumnNames="id"
                                 referencedTableName="ad_organization"/>

        <addForeignKeyConstraint baseColumnNames="vendor_id"
                                 baseTableName="m_warning_letter"
                                 constraintName="fk_m_warning_letter_vendor_id"
                                 referencedColumnNames="id"
                                 referencedTableName="c_vendor"/>

    </changeSet>

    <changeSet id="20210707092500-rel-1" author="ricky-k">
        <addForeignKeyConstraint baseColumnNames="business_category_id"
                                 baseTableName="m_warning_letter"
                                 constraintName="fk_m_warning_letter_business_category_id"
                                 referencedColumnNames="id"
                                 referencedTableName="c_business_category"/>

        <addForeignKeyConstraint baseColumnNames="sub_business_category_id"
                                 baseTableName="m_warning_letter"
                                 constraintName="fk_m_warning_letter_sub_business_category_id"
                                 referencedColumnNames="id"
                                 referencedTableName="c_business_category"/>
    </changeSet>
</databaseChangeLog>
