<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <!--
        Added the constraints for entity MVendorConfirmationLine.
    -->
    <changeSet id="20210527050000-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="ad_organization_id"
                                 baseTableName="m_vendor_confirmation_line"
                                 constraintName="fk_m_vendor_confirmation_line_ad_organization_id"
                                 referencedColumnNames="id"
                                 referencedTableName="ad_organization"/>

        <addForeignKeyConstraint baseColumnNames="vendor_id"
                                 baseTableName="m_vendor_confirmation_line"
                                 constraintName="fk_m_vendor_confirmation_line_vendor_id"
                                 referencedColumnNames="id"
                                 referencedTableName="c_vendor"/>

        <addForeignKeyConstraint baseColumnNames="bidding_eval_result_id"
                                 baseTableName="m_vendor_confirmation_line"
                                 constraintName="fk_m_vendor_confirmation_line_bidding_eval_result_id"
                                 referencedColumnNames="id"
                                 referencedTableName="m_bidding_eval_result"/>

        <addForeignKeyConstraint baseColumnNames="vendor_confirmation_id"
                                 baseTableName="m_vendor_confirmation_line"
                                 constraintName="fk_m_vendor_confirmation_line_vendor_confirmation_id"
                                 referencedColumnNames="id"
                                 referencedTableName="m_vendor_confirmation"/>

    </changeSet>
</databaseChangeLog>
