  <process id="vendorApproval" name="vendorApproval" isExecutable="true">
    <startEvent id="startevent1" name="Start"></startEvent>
    <userTask id="approveVendor" name="Approve Vendor" activiti:assignee="${vendorId}"></userTask>
    <sequenceFlow id="flow1" sourceRef="startevent1" targetRef="approveVendor"></sequenceFlow>
    <exclusiveGateway id="isVendorApproved" name="vendorApproved"></exclusiveGateway>
    <sequenceFlow id="flow2" sourceRef="approveVendor" targetRef="isVendorApproved"></sequenceFlow>
    <serviceTask id="updateVendorApproved" name="Update Vendor Approved" activiti:expression="${cvendorApprovalService.approved(execution)}" activiti:resultVariableName="documentStatus"></serviceTask>
    <sequenceFlow id="flow3" name="Approve" sourceRef="isVendorApproved" targetRef="updateVendorApproved">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${approve}]]></conditionExpression>
    </sequenceFlow>
    <serviceTask id="updateVendorRejected" name="Update Vendor Rejected" activiti:expression="${cvendorApprovalService.rejected(execution)}" activiti:resultVariableName="documentStatus"></serviceTask>
    <sequenceFlow id="flow4" name="Rejected" sourceRef="isVendorApproved" targetRef="updateVendorRejected">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${!approve}]]></conditionExpression>
    </sequenceFlow>
    <serviceTask id="updateDocumentStatus" name="Update Document Status" activiti:expression="${cvendorApprovalService.updateDocumentStatus(execution)}"></serviceTask>
    <exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway"></exclusiveGateway>
    <endEvent id="endevent1" name="End"></endEvent>
    <sequenceFlow id="flow9" name="Approved" sourceRef="exclusivegateway1" targetRef="endevent1">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${approve}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow10" name="Rejected" sourceRef="exclusivegateway1" targetRef="approveVendor">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${!approve}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow11" sourceRef="updateDocumentStatus" targetRef="exclusivegateway1"></sequenceFlow>
    <serviceTask id="sendApprovalEmail" name="Send Approval Email" activiti:expression="${cvendorApprovalService.approveEmail(execution)}"></serviceTask>
    <sequenceFlow id="flow12" sourceRef="updateVendorApproved" targetRef="sendApprovalEmail"></sequenceFlow>
    <sequenceFlow id="flow13" sourceRef="sendApprovalEmail" targetRef="updateDocumentStatus"></sequenceFlow>
    <serviceTask id="rejectEmail" name="Send Rejection Email" activiti:expression="${cvendorApprovalService.rejectEmail(execution)}"></serviceTask>
    <sequenceFlow id="flow14" sourceRef="updateVendorRejected" targetRef="rejectEmail"></sequenceFlow>
    <sequenceFlow id="flow15" sourceRef="rejectEmail" targetRef="updateDocumentStatus"></sequenceFlow>
  </process>
