	<parameter name="verificationId" class="java.lang.Long"/>
	<queryString language="SQL">
		<![CDATA[SELECT a.verification_no, a.verification_date, a.invoice_no, a.invoice_date, a.tax_invoice, a.tax_date, a.due_date, a.total_lines, a.tax_amount, a.grand_total,
            numberToWords(CAST(substring(CAST(a.grand_total AS text), 1, length(CAST(a.grand_total AS text))-3) AS bigint)) as terbilang,
            b.po_no, b.receive_no, b.description, b.qty, b.price_actual, b.total_lines AS total_line, b.tax_amount AS tax_amt, b.pay_stat,
            c.code AS code_currency,
            d.name AS vendor_name,d.tax_id_no AS npwp
        FROM m_verification AS a,
            m_verification_line AS b,
            c_currency AS c,
            c_vendor AS d
        WHERE
